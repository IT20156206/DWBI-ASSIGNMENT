  <DataSets>
    <DataSet Name="Drill_down_report_DataSet">
      <Query>
        <DataSourceName>Drill_down_report_DataSource</DataSourceName>
        <CommandText>SELECT dde.AlternateID, dde.Department_Name, dde.Ward_Type, dl.Country, dl.City, dl.address, ddo.first_name, ddo.last_name, ddo.speciality, dd.Month, dd.MonthName, dd.Year, fh.Hospital_Name, fh.Hospital_Type, fh.Hospital_Ownership, 
                  fh.patient_id, fh.Patient_City, fh.ContactNo, fh.Patient_Visitors
FROM     FactHospital AS fh INNER JOIN
                  DimDepartment AS dde ON fh.DepartmentKey = dde.DepartmentSK INNER JOIN
                  DimLocation AS dl ON fh.LocationKey = dl.LocationSK INNER JOIN
                  DimDoctor AS ddo ON fh.DoctorKey = ddo.DoctorSK INNER JOIN
                  DimDate AS dd ON fh.Discharge_DateKey = dd.DateKey</CommandText>
      </Query>
